
@{
	Layout = null;
}

@model IEnumerable<SportStore.Models.CartLine>





@using(@Html.BeginForm("Checkout", "Cart", new{}, FormMethod.Post))
{
	@Html.AntiForgeryToken()
    for(int i = 0; i < Model.Count(); i++){
		@Html.HiddenFor(m => m.ElementAt(i).CartLineId)
    }
	<input type="submit" name="Checkout" value="Checkout">
}

